import java.util.ArrayList;

public class ArraylistDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		ArrayList arrayNumbers = new ArrayList();
		arrayNumbers.add(8);
		arrayNumbers.add(3);
		arrayNumbers.add("name");
		arrayNumbers.add(5.4f);
		//print, ham size tra ve kich co cua mang
		for(int i = 0; i < arrayNumbers.size(); i++) {
			System.out.println(arrayNumbers.get(i));
			
		}
		for(int i = 0; i < arrayNumbers.size(); i++) {
		System.out.println(" index 0:" + arrayNumbers.get(0));
		
	}}

}
