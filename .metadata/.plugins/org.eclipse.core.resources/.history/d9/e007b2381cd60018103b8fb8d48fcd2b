package OOPPart1;
//su dung cho ke thua


public class NhanVat {
	
	public String ten;
    public int Mau;
	public int satThuong;

	 public void display() {
		   System.out.println(" ten nhan vat la: " + this.ten);
		   System.out.println(" mau cua nhan vat: " + this.Mau);
		   System.out.println(" Do sat thuong la: " + this.satThuong);
          }
	 }